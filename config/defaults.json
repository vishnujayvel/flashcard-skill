{
  "output": {
    "base_dir": "~/.local/share/flashcard-skill",
    "format": "both",
    "auto_save": true,
    "naming": "{topic}/{date}.{ext}"
  },
  "formats": {
    "csv": {
      "include_header": false,
      "quote_fields": true
    },
    "markdown": {
      "include_metadata": true,
      "card_separator": "---",
      "question_prefix": "**Q:**",
      "answer_prefix": "**A:**"
    }
  },
  "index": {
    "enabled": true,
    "file": "index.json"
  }
}
